generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model WorkLog {
  id         Int      @id @default(autoincrement())
  date       DateTime
  driver     String
  customer   String
  client     String
  startTime  String
  finishTime String
  truckType  String
  vehicle    String
  comments   String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
